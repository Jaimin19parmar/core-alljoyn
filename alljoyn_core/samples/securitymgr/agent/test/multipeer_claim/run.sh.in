INSTALL_ROOT="%PREFIX%"

export LD_LIBRARY_PATH="%LIBPATH%"

if [ -z "$1" ]; then
    if [ -z "${NUM_PEERS}" ]; then
        NUM_PEERS=3
    fi
    ${DEBUG} "${INSTALL_ROOT}/%TESTAPP%" ${NUM_PEERS}

    RC=$?
    echo "Exit code ${RC}"
    exit ${RC}
else
    ${DEBUG} "${INSTALL_ROOT}/%TESTAPP%" "$@"
fi